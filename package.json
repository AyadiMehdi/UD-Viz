{
  "name": "udv",
  "version": "2.28.1",
  "description": "A collection of itowns plugins",
  "main": "src/udv.js",
  "scripts": {
    "lint": "eslint \"src/**/*.js\"",
    "doc": "jsdoc UDV.js -- -t templates/silent",
    "build": "npm run build-dev",
    "build-dev": "webpack --config webpack.config.js",
    "dev": "nodemon --verbose --watch src --delay 2500ms ./bin/dev.js -e js,css,html",
    "build-server": "webpack --config webpack-server.config.js"
  },
  "files": [
    "Tools",
    "Modules"
  ],
  "repository": {

    "url": "git+https://github.com/MEPP-team/UD-Viz.git"
  },
  "license": "(LGPL-2.1)",
  "bugs": {
    "url": "https://github.com/MEPP-team/UD-Viz/issues"
  },
  "homepage": "https://github.com/MEPP-team/UD-Viz/",
  "dependencies": {
    "itowns": "2.28.1",
    "proj4": "^2.6.2",
    "three": "^0.122.0",
    "vis-data": "^7.1.2",
    "vis-network": "^9.0.3"
  },
  "devDependencies": {
    "child-process-promise": "^2.2.1",
    "css-loader": "^0.28.10",
    "nodemon": "^2.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.5.0"
  }

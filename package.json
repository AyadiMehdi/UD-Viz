{
  "name": "ud-viz",
  "version": "2.32.4",
  "description": "A collection of itowns plugins",
  "main": "src",
  "scripts": {
    "watch": "cross-env BABEL_DISABLE_CACHE=1 babel --watch src --out-dir lib",
    "transpile": "cross-env BABEL_DISABLE_CACHE=1 babel src --out-dir lib --copy-files",
    "test-babel": "./node_modules/.bin/babel test --out-dir lib-test",
    "test": "npm run test-babel && mocha ./lib-test/**/*.js -r ignore-styles -r @babel/register --file lib-test/unit/test.js",
    "mocha": "mocha -r @babel/register -r ignore-styles -r jsdom-global/register ./lib-test/**/*.js",
    "transpile-test": "npm run transpile && npm run test",    
    "travis": "npm run eslint && npm run build",
    "eslint": "./node_modules/.bin/eslint ./src",
    "eslint-fix": "./node_modules/.bin/eslint ./src --fix",
    "build": "cross-env NODE_ENV=production webpack",
    "build-debug": "cross-env NODE_ENV=development webpack",
    "debug": "nodemon --verbose --watch src --delay 2500ms ./bin/debug.js -e js,css,html"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VCityTeam/UD-Viz.git"
  },
  "license": "(LGPL-2.1)",
  "bugs": {
    "url": "https://github.com/VCityTeam/UD-Viz/issues"
  },
  "homepage": "https://github.com/VCityTeam/UD-Viz/",
  "peerDependencies": {},
  "dependencies": {
    "detect-collisions": "^2.5.2",
    "itowns": "2.33.0",
    "jquery": "^3.6.0",
    "proj4": "^2.7.0",
    "socket.io-client": "^4.0.0",
    "three": "0.127.0",
    "three-css3drenderer": "^1.0.1",
    "vis-data": "^7.1.2",
    "vis-network": "^9.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "chai": "^4.3.4",
    "child-process-promise": "^2.2.1",
    "core-js": "^3.15.2",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.10",
    "eslint": "^7.28.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.0.2",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0"
  }
}
